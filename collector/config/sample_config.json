{
	"source" : "http://localhost:8000/nginx_status",
	"polling_frequency" : 10,
	"metrics_config"  : [
		{ "name" : "activeConnections",  
		  "metric_type" : "gauge",
		  "units" : "integer",
		  "regex" : "Active connections: ([0-9]+)"			
		},
		{ "name" : "reading",
		  "metric_type" : "gauge", 
		  "units" : "integer",
		  "regex" : "Reading: ([0-9]+) .*"	
		},
		{ "name" : "writing",
                  "metric_type" : "gauge",
                  "units" : "integer",
                  "regex" : ".*Writing: ([0-9]+).*"
                },
		{ "name" : "waiting",
                  "metric_type" : "gauge",
                  "units" : "integer",
                  "regex" : ".*Waiting: ([0-9]+)"
                }
	] 	
}
